@model ViewModelCoreMVC.ViewModels.EmployeeDetails
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@Model?.Title</title>
</head>
<body>
    <h1>@Model?.Header</h1>
    <div>
        EmployeeId:@Model?.Employee?.EmployeeId
    </div>
    <div>
       FirstName: @Model?.Employee?.FirstName
    </div>
    <div>
        LastName:@Model?.Employee?.LastName
    </div>
    <div>
       Email: @Model?.Employee?.Email
    </div>
    <div>
       PhoneNumber: @Model?.Employee?.Phone
    </div>
    <div>
      Salary:   @Model?.Employee?.Salary
    </div>
    <h1>Department Details</h1>
    <div>
        EmployeeId:@Model?.Department?.EmployeeId
    </div>
    <div>
       DepName :@Model?.Department?.DepName
    </div>
    <div>
       DomainName :@Model?.Department?.DomainName
    </div>
    <div>
        JoinDate:@Model?.Department?.JoinDate
    </div>
</body>
</html>
